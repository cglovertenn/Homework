Sub ticksandtots_2016()Dim ticker As StringDim totalvolume As DoubleDim currentrow As Integercurrentrow = 2For i = 2 To 797711    totalvolume = totalvolume + Cells(i, 7).Value    If Cells(i + 1, 1).Value <> Cells(i, 1).Value Then    ticker = Cells(i, 1).Value    Range("I" & currentrow).Value = ticker    Range("J" & currentrow).Value = totalvolume        totalvolume = 0    currentrow = currentrow + 1        End If    Next iEnd Sub

